{
  "name": "@toddpa/shared-module",
  "version": "0.1.12",
  "description": "A template for a shared module",
  "author": "Paul Todd",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "build": "npm run tsc && npm test",
    "watch": "tsc -w",
    "test": "JASMINE_CONFIG_PATH=jasmine.json jasmine-ts \"./src/**/*.spec.ts\"",
    "run": "ts-node ./src/common/index.ts",
    "cover": "JASMINE_CONFIG_PATH=jasmine.json istanbul cover node_modules/jasmine/bin/jasmine.js",
    "test:coverage": "ts-node node_modules/istanbul/lib/cli.js cover -e .ts  -x \"*.d.ts\" -x \"*.spec.ts\" node_modules/jasmine/bin/jasmine.js -- JASMINE_CONFIG_PATH=jasmine.json"
 },
  "types": "index.d.ts",
  "main": "index.js",
  "dependencies": {
    "@types/gaussian": "^1.1.1",
    "gaussian": "^1.1.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.53",
    "istanbul": "^1.1.0-alpha.1",
    "jasmine": "^2.5.2",
    "jasmine-spec-reporter": "^4.1.1",
    "jasmine-ts": "^0.1.3",
    "ts-loader": "^2.1.0",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "typescript": "2.4.0",
    "typings": "^2.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/toddpa/srt-common.git"
  },
  "keywords": [
    "typescript",
    "vscode"
  ]
}
